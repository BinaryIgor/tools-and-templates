{
  "volumes": [
    "$PWD/conf:/etc/nginx/conf.d"
  ],
  "build_cmd": "bash build.bash",
  "build_env": {
    "HTTP_PORT": 80,
    "HTTPS_PORT": 80
  },
  "pre_run_cmd": "bash update_app_url_pre_start.bash ${current-app-url-file-path}",
  "post_run_cmd": "bash check_proxied_app.bash",
  "network": "host",
  "memory": "500M",
  "cpus": "0.5"
}