                    "rawSql": "SELECT\n  name,\n  rounded_timestamp,\n  pg_size_pretty(used_memory) as used_memory,\n  pg_size_pretty(max_memory) as max_memory,\n  concat(((used_memory * 100 / max_memory)::bigint)::text, '%') as memory_usage,\n  concat(((cpu_usage * 100)::bigint)::text, '%') as cpu_usage\nFROM (\n  SELECT name,\n   (date_trunc('hour', timestamp) + date_part('minute', timestamp)::int / 5 * interval '5 min') as \"rounded_timestamp\",\n   AVG(used_memory) as used_memory,\n   AVG(max_memory) as max_memory,\n   AVG(cpu_usage) as cpu_usage\n  FROM application_state\n  WHERE name = '$application' AND $__timeFilter(timestamp)\n  GROUP BY name, rounded_timestamp\n) as application_average_state\nORDER BY rounded_timestamp DESC;",
