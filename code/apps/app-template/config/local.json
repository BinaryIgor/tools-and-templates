{
  "volumes": [
    "${SECRETS_VOLUME_PATH}:/secrets:ro"
  ],
  "build_cmd": "bash build_cmd.bash",
  "env": {
    "JVM_OPTS": "-Xmx450m",
    "ENV": "local",
    "DB_URL": "${db-url}",
    "DB_USER": "${db-user}",
    "DB_PASSWORD": "file:/secrets/db-password.txt",
    "JWT_TOKEN_KEY": "file:/secrets/jwt-token-key.txt"
  },
  "secrets": {},
  "network": "host",
  "memory": "500M"
}